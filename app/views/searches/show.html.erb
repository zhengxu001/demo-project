<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @search.name %>
</p>

<p>
  <strong>Query:</strong>
  <%= @search.query %>
</p>

<p>
  <strong>Results:</strong>
  <% @results.each do |result| %>
  	<% @results.variable_names.each do |var| %>
  	  <li><%= link_to result[var], result[var].to_s %></li>
  	<% end %>
  <% end %>
</p>


<%= link_to 'Edit', edit_search_path(@search) %> |
<%= link_to 'Back', searches_path %>
